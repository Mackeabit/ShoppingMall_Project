<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>
	
	<typeAliases>
	
		<typeAlias type="vo.UserVO" alias="user"/>
		<typeAlias type="vo.MyPageVO" alias="mypage"/>
		<typeAlias type="vo.OrderViewVO" alias="orderView"/>
		<typeAlias type="vo.CartVO" alias="cart"/>
		<typeAlias type="vo.AdminVO" alias="admin"/>
		<typeAlias type="vo.OrderVO" alias="order"/>
		<typeAlias type="vo.ProductVO" alias="product"/>
		<typeAlias type="vo.MainEventVO" alias="main"/>
		<typeAlias type="vo.All_productVO" alias="All_product"/>
		<typeAlias type="vo.CpuViewVO" alias="cpu"/>
		<typeAlias type="vo.CategoryVO" alias="category"/>
		<typeAlias type="vo.ROviewVO" alias="roview"/>
		<typeAlias type="vo.PayVO" alias="pay"/>
		<typeAlias type="vo.CancleVO" alias="cancle"/>
		<typeAlias type="vo.QnaVO" alias="qna"/>
		<typeAlias type="vo.PdQnaViewVO" alias="pdqnaview"/>
		<typeAlias type="vo.PdreviewVO" alias="pdreview"/>
		<typeAlias type="vo.QnaViewVO" alias="qnaView"/>
		<typeAlias type="vo.ReviewVO" alias="review"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="config/mybatis/mapper/user.xml" />
		<mapper resource="config/mybatis/mapper/order.xml" />
		<mapper resource="config/mybatis/mapper/cart.xml" />
		<mapper resource="config/mybatis/mapper/review.xml" />
		<mapper resource="config/mybatis/mapper/qna.xml" />
		<mapper resource="config/mybatis/mapper/product.xml" />
		<mapper resource="config/mybatis/mapper/roview.xml" />
		<mapper resource="config/mybatis/mapper/pay.xml" />
		<mapper resource="config/mybatis/mapper/views.xml" />
		
		<mapper resource="config/mybatis/mapper/admin.xml" />
		<mapper resource="config/mybatis/mapper/admin_user.xml" />
		<mapper resource="config/mybatis/mapper/admin_sales.xml" />
		<mapper resource="config/mybatis/mapper/pd_sales.xml" />
		<mapper resource="config/mybatis/mapper/admin_order.xml" />
		<mapper resource="config/mybatis/mapper/order_num.xml" />
		<mapper resource="config/mybatis/mapper/admin_product.xml" />
		<mapper resource="config/mybatis/mapper/admin_qna.xml" />
		<mapper resource="config/mybatis/mapper/admin_review.xml" />
	</mappers>
</configuration>